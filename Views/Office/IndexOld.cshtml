@model  AspNetUser
@using Microsoft.AspNet.Identity
@using Marofh.Models

@{
    ViewBag.Title = " مكتب الاستقدام";
    ViewBag.Sub1Title = "حسابى";
    ViewBag.Sub1Url = "/office/";
    Layout = "~/Views/Shared/_LayoutInner.cshtml";
}


@{
    string userId = User.Identity.GetUserId();
    AspNetUser loggedInUser = ViewModel.GetCurrentUser(User.Identity.GetUserId());

    int officeRoleId = Convert.ToInt32(clsEnum.Roles.Office);
    int workerRoleId = Convert.ToInt32(clsEnum.Roles.Worker);
}

<div class="container mt-4">
    <div class="row">

        <div class="col-12 col-md-5 col-lg-4 col-xl-3">
            <!-- user -->
            <div class="user">
                <div class="user__head">
                    <div class="user__img">
                        @if (!string.IsNullOrEmpty(Model.ImageUrl))
                        {
                            <img src="@Model.ImageUrl" alt="">
                        }
                        else
                        {
                            <img src="/Content/front/assets/img/user.png" alt="">
                        }

                        <a href="javascript:void(0)" class="imgcss" onclick="editOfficeImageProfile(@loggedInUser.ShortID)">

                            <i class="fa fa-camera fa-5x"></i>
                            <span class="changetitle">تغيير الصورة</span>
                        </a>
                    </div>

                </div>


                <div class="user__title">
                    <h2>@Model.OfficeName</h2>
                    <p>@Model.Country.NameAR</p>
                    <div class="verviedaccoutn ">
                        @if (ViewModel.IsVerifiedOffice(User.Identity.GetUserId()))
                        {
                            <i class="fa fa-check-circle"></i>

                        }
                        else
                        {
                            <i class="fas fa-times-circle"></i> @Html.Raw("غير موثق")
                            if (!string.IsNullOrEmpty(Model.ContractUrl))
                            {
                                <h2>العقد تحت المراجعة </h2>

                            }

                        }
                    </div>

                    <div class=" mt-20 staticite">
                        <div class="cright">
                            <span class="fad fa-user-circle d-block"></span>
                            <span class="d-block fontsizerbol">
                                @Marofh.Models.ViewModel.GetWorkersCount(userId, workerRoleId)
                            </span>
                            <span class="cfont">
                                العاملات
                            </span>

                        </div>
                        @*<div class="cright">
                                <span class="fad fa-eye d-block"></span>
                               <span class="d-block fontsizerbol">200</span>
                               <span class="cfont">
                                   المشاهدات
                               </span>
                            </div>*@


                    </div>


                </div>
                <p class="cright">

                    <span class="far fa-clock "></span> &nbsp;
                    <strong>اخر دخول : </strong>
                    @if (Model.LastLoggedInDate.HasValue)
                    {
                        @Model.LastLoggedInDate.Value.ToShortDateString();
                    }
                    else
                    {

                    }


