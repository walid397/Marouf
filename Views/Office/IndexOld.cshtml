                );

                marker.setMap(null);

                marker = new google.maps.Marker({
                    position: mapsMouseEvent.latLng,
                    map: map,
                    icon: icon
                });


                //alert(JSON.stringify(mapsMouseEvent.latLng.toJSON(), null, 2));
                var location = (JSON.stringify(mapsMouseEvent.latLng.toJSON(), null, 2)).split(',');

                var Longitude = document.getElementById("Longitude");
                Longitude.value = location[1].trim().replace("lng\": ", "").replace("}", "").replace("\"", "");

                var Latitude = document.getElementById("Latitude");
                Latitude.value = location[0].trim().replace("lat\": ", "").replace("{", "").trim().replace("\"", "");



                infoWindow.open(map, marker);


            });
            initMap1();
        }

