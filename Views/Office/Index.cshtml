@model  AspNetUser
@using Microsoft.AspNet.Identity
@using Marofh.Models
@using Resources

@{
    var culture = System.Threading.Thread.CurrentThread.CurrentUICulture.Name.ToLowerInvariant();
    if (culture.Contains("en-us"))
    {

        ViewBag.Title = " Recruiting Office ";
        ViewBag.Sub1Title = "My Account";

    }
    else
    {

        ViewBag.Title = " مكتب الاستقدام";
        ViewBag.Sub1Title = "حسابى";

    }
    <!-- ✅ CSS -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/Content/front/assets/js/datatable/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="/Content/front/assets/css/select2.min.css" rel="stylesheet" />
    <link href="/Scripts/sweetalert/sweetalert2.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/11.9.0/styles/default.min.css" />


    ViewBag.Sub1Url = "/office/";
    Layout = "~/Views/Shared/_LayoutInner.cshtml";
}


@{
    string userId = User.Identity.GetUserId();
