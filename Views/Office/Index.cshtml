}

<div class="container mt-4">
    <div class="row">

        <div class="col-12 col-md-5 col-lg-4 col-xl-3">
            <!-- user -->
            <div class="user">
                <div class="user__head">
                    <div class="user__img">
                        @if (!string.IsNullOrEmpty(Model.ImageUrl))
                        {
                            <img src="@Model.ImageUrl" alt="">
                        }
                        else
                        {
                            <img src="/Content/front/assets/img/user.png" alt="">
                        }

                        <a href="javascript:void(0)" class="imgcss" onclick="editOfficeImageProfile(@loggedInUser.ShortID)">

                            <i class="fa fa-camera fa-5x"></i>
                            <span class="changetitle">@Resource.ChangeImage  </span>
                        </a>
                    </div>

                </div>


                <div class="user__title">

                    <h2>@Model.OfficeName</h2>
                    @if (culture.Contains("en-us"))
                    {
                        <p>@Model.Country.NameEN</p>

                    }
                    else
                    {
                        <p>@Model.Country.NameAR</p>

                    }




                    <div class="verviedaccoutn text-center mb-2">
                        @if (Convert.ToBoolean(Model.IsVerifed))
                        {
                            <i class="fa fa-check-circle"></i>

                        }


                        else
                        {
                            <i class="fas fa-times-circle text-danger"></i> <span class="text-danger">@Html.Raw(Resource.Undocumented)</span>
                            if (!string.IsNullOrEmpty(Model.ContractUrl))
                            {
                                <h3 class="d-block mt-2 badge badge-warning"> @Resource.ContractUnderReview</h3>

                            }

                        }
